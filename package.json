{
  "name": "hp_ext",
  "displayName": "HP Browser Extension",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "description": "hp's browser extension",
  "repository": {
    "type": "git",
    "url": "https://github.com/hariprasadr1hp/hp_ext"
  },
  "engines": {
    "node": ">=23",
    "pnpm": ">=9"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:chrome": "cross-env TARGET_BROWSER=chrome vite build",
    "build:firefox": "cross-env TARGET_BROWSER=firefox vite build",
    "build:all": "pnpm run build:chrome && pnpm run build:firefox",
    "clean": "rm -rf dist/",
    "clean-install": "rm -rf node_modules/ package-lock.json && pnpm i",
    "lint": "eslint . --ext .ts --fix",
    "lint:manifest": "web-ext lint --pretty",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@casl/ability": "^6.7.3",
    "@mdi/font": "^7.4.47",
    "@pola-rs/browser": "0.0.1-alpha",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "ag-grid-community": "^33.2.1",
    "ag-grid-vue3": "^33.2.1",
    "axios": "^1.8.4",
    "jwt-decode": "^4.0.0",
    "lightweight-charts": "^5.0.5",
    "marked": "^15.0.7",
    "nodejs-polars": "^0.18.0",
    "pinia": "^3.0.1",
    "plotly.js-dist-min": "^3.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rollup-plugin-chrome-extension": "^3.6.12",
    "sass": "^1.86.1",
    "svelte": "^5.25.6",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuetify": "3.8.0"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.0.0-beta.25",
    "@types/chrome": "^0.0.313",
    "@types/node": "^22.13.16",
    "@types/webextension-polyfill": "^0.12.3",
    "@types/webfontloader": "^1.6.38",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/compiler-sfc": "^3.5.13",
    "axios-mock-adapter": "^2.1.0",
    "cross-env": "^7.0.3",
    "eslint": "9.23.0",
    "terser": "^5.39.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.4",
    "vite-plugin-pages": "^0.33.0",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue-tsc": "^2.2.8",
    "web-ext": "^8.5.0",
    "webext-bridge": "^6.0.1"
  },
  "packageManager": "pnpm@9.4.0+sha512.f549b8a52c9d2b8536762f99c0722205efc5af913e77835dbccc3b0b0b2ca9e7dc8022b78062c17291c48e88749c70ce88eb5a74f1fa8c4bf5e18bb46c8bd83a"
