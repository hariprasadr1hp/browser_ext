root = true

[*]
charset = utf-8
indent_style = space
end_of_line = lf
trim_trailing_whitespace = true
quote_type = double
insert_final_newline = true

[*.css]
indent_size = 2

[*.js]
indent_size = 2

[*.json]
indent_size = 2

[*.jsx]
indent_size = 2

[*.scss]
indent_size = 2

[*.svelte]
indent_size = 2

[*.ts]
indent_size = 2

[*.tsx]
indent_size = 2

[*.vue]
indent_size = 2
